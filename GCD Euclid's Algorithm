#include <stdio.h>
 

int gcd(int a, int b)
{
    if (a == 0)
        return b;
    return gcd(b%a, a);
}
 
// Driver program to test above function
int main()
{
    int a = 12, b = 12;
    printf(" Euclids algorithm\n");
    printf("2005638");
    printf("\nGCD(%d, %d) = %d\n", a, b, gcd(a, b));
    
    return 0;
}
